BUFFER_SIZE	client.h	12;"	d
CLIENT_FIFO_NAME	client.h	10;"	d
FIFO_NAME	fifo2.c	8;"	d	file:
SERVER_FIFO_NAME	client.h	9;"	d
client_pid	client.h	/^    pid_id client_pid;$/;"	m	struct:data_to_pass_st
data_to_pass_st	client.h	/^struct data_to_pass_st{$/;"	s
main	fifo2.c	/^int main(int argc, char *argv[])$/;"	f
main	server.c	/^int main(void)$/;"	f
some_data	client.h	/^    char some_data[BUFFER_SIZE -1];$/;"	m	struct:data_to_pass_st
